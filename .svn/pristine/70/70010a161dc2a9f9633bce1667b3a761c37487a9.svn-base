<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="arun.com.attendancesystem"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="arun.com.attendancesystem.AttendanceActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="arun.com.attendancesystem.AddBatchActivity" />
        <activity
            android:name="arun.com.attendancesystem.ActionsActivity"
            android:label="@string/app_name" />
        <activity android:name="arun.com.attendancesystem.student.AddStudentActivity" />
        <activity android:name="arun.com.attendancesystem.MarkAttendance" />
        <activity android:name="arun.com.attendancesystem.CalendarView" />
        <activity android:name="arun.com.attendancesystem.CalculateFeesActivity" />
    </application>

</manifest>